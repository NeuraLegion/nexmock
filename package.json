{
  "name": "@neuralegion/nexmock",
  "version": "1.1.3",
  "description": "Mock http requests made using fetch (or isomorphic-fetch)",
  "main": "src/server.js",
  "browser": "es5/client.js",
  "scripts": {
    "test": "make lint test",
    "prepublishOnly": "make transpile bundle",
    "postinstall": "node scripts/support-fetch-mock.js"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "useBuiltIns": "entry"
        }
      ]
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": true
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NeuraLegion/nexmock.git"
  },
  "keywords": [
    "fetch",
    "http",
    "mock",
    "testing",
    "spy",
    "xhr",
    "ajax",
    "nexploit"
  ],
  "author": "Artem Derevnjuk <artem.derevnjuk@neuralegion.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NeuraLegion/nexmock/issues"
  },
  "homepage": "https://github.com/NeuraLegion/nexmock#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "browser-or-node": "^1.2.1",
    "glob-to-regexp": "^0.4.1",
    "path-to-regexp": "^3.0.0",
    "whatwg-url": "^7.0.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "devDependencies": {
    "@neuralegion/form-data": "^2.5.2",
    "@neuralegion/karma-nexmock-reporter": "^1.0.2",
    "@neuralegion/mocha-nexmock-reporter": "^1.0.3",
    "babel-cli": "6.26.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "bluebird": "^3.5.5",
    "chai": "^4.2.0",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-origami-component": "^1.0.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "isomorphic-fetch": "^2.2.1",
    "karma": "^4.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "mocha": "^6.1.4",
    "node-fetch": "^3.1.1",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3"
  }
}
